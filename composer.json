{
    "name": "saperx/laravel-editorjs-html",
    "keywords": [
        "laravel",
        "editorjs",
        "convert",
        "html"
    ],
    "description": "Convert object editorjs to html view",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "codex-team/editor.js": "dev-master",
        "illuminate/support": "^6|^7|^8"
    },
    "authors": [
        {
            "name": "Muntadher",
            "email": "dev@ankitech.co"
        },
        {
            "name": "Willian de Souza",
            "email": "willian.souza@saperx.com.br"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SaperX\\LaravelEditorjsHtml\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SaperX\\LaravelEditorjsHtml\\EditorJSHtmlProvider"
            ],
            "aliases": {
                "EditorJSHtml": "SaperX\\LaravelEditorjsHtml\\Facades\\EditorJSHtml"
            }
        }
    }
}